<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="556.0" prefWidth="793.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controller.TelaListagemVendasController">
   <children>
      <Button fx:id="btList" layoutX="149.0" layoutY="486.0" mnemonicParsing="false" onAction="#listSales" prefHeight="38.0" prefWidth="100.0" text="Listar" />
      <Button fx:id="btCancel" layoutX="559.0" layoutY="486.0" mnemonicParsing="false" onAction="#cancel" prefHeight="38.0" prefWidth="100.0" text="Cancelar" />
      <TableView fx:id="tableSales" layoutX="10.0" layoutY="10.0" prefHeight="459.0" prefWidth="772.0">
         <columns>
            <TableColumn fx:id="tableId" maxWidth="4000.0" prefWidth="116.0" text="Codigo" />
            <TableColumn fx:id="tableClient" minWidth="5.0" prefWidth="198.0" text="Cliente (ID)" />
            <TableColumn fx:id="tableBooks" prefWidth="171.0" text="Livros (ID)" />
            <TableColumn fx:id="tableValue" maxWidth="3000.0" prefWidth="118.0" text="Valor" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
